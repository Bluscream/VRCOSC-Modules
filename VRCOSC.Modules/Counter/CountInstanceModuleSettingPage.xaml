<Page x:Class="VRCOSC.Modules.Counter.CountInstanceModuleSettingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:ui="clr-namespace:VRCOSC.App.UI;assembly=VRCOSC.App"
      mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <ui:VRCOSCButton ButtonColour="Blue" Click="Button_OnClick">
            <TextBlock
                FontSize="14"
                Foreground="{StaticResource CForeground1}"
                TextAlignment="Center"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Text="Edit Count Instances"/>
        </ui:VRCOSCButton>
    </Grid>
</Page>
